{
  "name": "react-start",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:client": "NODE_ENV=development parcel -p 3002 client/index.html",
    "dev:server": "NODE_ENV=development nodemon server/index.js",
    "dev": "concurrently 'npm run dev:client' 'npm run dev:server'",
    "build": "NODE_ENV=build parcel build --no-cache --no-source-maps client/index.html",
    "start": "NODE_ENV=production node server/index.js",
    "start-prod:test": "NODE_ENV=test nodemon server/index.js",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "test-debug": "NODE_ENV=test jest --verbose --runInBand --forceExit",
    "test": "NODE_ENV=test jest --verbose --forceExit",
    "cypress:open": "cypress open"
  },
  "proxy": "http://localhost:3001",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.32.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.1",
    "redux-thunk": "^2.4.1",
