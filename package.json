{
  "name": "react-start",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:client": "parcel -p 3002 client/index.html",
    "dev:server": "NODE_ENV=development nodemon server/index.js",
    "dev": "concurrently 'npm run dev:client' 'npm run dev:server'",
    "build": "NODE_ENV=build parcel build --no-cache --no-source-maps client/index.html",
    "start": "NODE_ENV=production node server/index.js",
    "start-prod:test": "NODE_ENV=test nodemon server/index.js",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "test-debug": "NODE_ENV=test jest --verbose --runInBand --forceExit",
    "test": "NODE_ENV=test jest --verbose --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.4.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.1.1",
    "nodemon": "^2.0.20",
    "parcel": "^2.6.2",
    "prettier": "^2.7.1",
    "process": "^0.11.10"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
